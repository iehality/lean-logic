@[simp] lemma forall_dummy (p : formula L m) : T ‚ä¢ ‚àÄ'ùóó p ‚ü∑ p :=
begin
  simp[iff_equiv], split,
  { have : T ‚ä¢ ‚àÄ'(‚ä§ ‚ü∂ ùóó p) ‚ü∂ ‚àÉ'‚ä§ ‚ü∂ p, from univ_imply_dummy ‚ä§ p,
    refine imply_of_equiv this (equiv_forall_of_equiv $ by simp) (by simp) },
